{
  "name": "ydn/tracking",
  "description": "Laravel package for ocean tracking API integration.",
  "type": "library",
  "keywords": ["laravel", "ocean", "tracking", "container", "logistics"],
  "license": "MIT",
  "authors": [
    {
      "name": "Andy Moxq",
      "email": "andy@outsofas.com"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Ocean\\Tracking\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Ocean\\Tracking\\Providers\\OceanTrackingServiceProvider"
      ]
    }
  }
}